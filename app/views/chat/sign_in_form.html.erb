<div class="sign_in_container">
  <h2> Who are you? </h2>
  <div class="input_container">
    <%=form_tag :action => :sign_in do %>
      <label for="username">Username</label>
      <input type="text" id="username" name="username"/>
      <input type="submit" value="Go" />
    <% end %>
  </div>
</div>
